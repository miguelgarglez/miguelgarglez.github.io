---
import ChatLauncher from './ChatLauncher';

const legacyWorkerUrl = import.meta.env.DEV
  ? import.meta.env.DEV_PUBLIC_WORKER_CHAT_URL
  : import.meta.env.PUBLIC_WORKER_CHAT_URL;

const primaryApiUrl = (
  import.meta.env.DEV
    ? import.meta.env.DEV_PUBLIC_CHAT_API_PRIMARY_URL
    : import.meta.env.PUBLIC_CHAT_API_PRIMARY_URL
) ?? legacyWorkerUrl ?? '';

const secondaryApiUrl = import.meta.env.DEV
  ? import.meta.env.DEV_PUBLIC_CHAT_API_SECONDARY_URL
  : import.meta.env.PUBLIC_CHAT_API_SECONDARY_URL;
---

<ChatLauncher
  primaryApiUrl={primaryApiUrl}
  secondaryApiUrl={secondaryApiUrl}
  client:load
/>
